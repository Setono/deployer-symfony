{
    "name": "setono/deployer-symfony",
    "type": "library",
    "description": "Install the symfony binary on your remote server",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.4",
        "composer-runtime-api": "^2",
        "composer/package-versions-deprecated": "^1.11",
        "deployer/deployer": "^6.8 || ^7.0"
    },
    "require-dev": {
        "setono/code-quality-pack": "^2.1.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\Deployer\\Symfony\\": "src/"
        },
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\Deployer\\Symfony\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix"
    }
}
